        $("#totalTue").text(tueTotal);
        $("#totalWed").text(wedTotal);
        $("#totalThu").text(thuTotal);
        $("#totalFri").text(friTotal);
        $("#totalSat").text(satTotal);
        $("#totalTotal").text(totalTotal);

