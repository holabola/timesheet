      function expenseSwitcher() {

          var CUList, nameListEX, CUListEX, DateListEX, approvalFinalEX, approvalFinal;
          CUList = [], nameListEX = [], CUListEX = [], DateListEX = [], approvalFinalEX = [], approvalFinal = [];

          <% @expenses.each do |e| %>
